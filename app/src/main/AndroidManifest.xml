<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.internshala.tejrajfooddelivery">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_appicon"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_appicon_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.activity.ConfirmOrderActivity"></activity>
        <activity android:name=".activity.activity.OtpActivity" />
        <activity android:name=".activity.activity.CartActivity" />
        <activity android:name=".activity.activity.MenuCardActivity" />
        <activity android:name=".activity.fragment.FaqFragment" />
        <activity android:name=".activity.fragment.OrderHistoryFragment" />
        <activity android:name=".activity.fragment.FavouriteFragment" />
        <activity android:name=".activity.fragment.MyProfileFragment" />
        <activity android:name=".activity.fragment.HomeFragment" />
        <activity android:name=".activity.activity.LogoActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.activity.RegisterActivity" />
        <activity android:name=".activity.activity.ForgotPasswordActivity" />
        <activity android:name=".activity.activity.LoginActivity" />
        <activity android:name=".activity.activity.MainActivity" />
    </application>

</manifest>